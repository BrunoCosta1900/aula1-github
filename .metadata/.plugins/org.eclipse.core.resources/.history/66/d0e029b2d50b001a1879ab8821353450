package entities;

public class product {

	public String name;
	public double price;
	public int quantity;

	public double tutalValueInStock() {
		return price * quantity;
	}
	
	public void addProducts(int quantity) {
		this.quantity += quantity;
	}
	public void eddProducts (int quantity) {
		this.quantity -= quantity;
		
	}
}
