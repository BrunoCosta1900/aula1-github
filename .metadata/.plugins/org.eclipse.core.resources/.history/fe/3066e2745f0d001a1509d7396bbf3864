package entities;

public class Banco {

	private int conta;
	private String nome;
	private double valorEmConta;

	public Banco(int conta, String nome, double depositoInicial(deposito){

		this.conta = conta;
		this.nome = nome;
		depositoInicial(desposito);
	}

	public Banco(int conta, String nome, double valorEmConta) {

		this.conta = conta;
		this.nome = nome;
		this.valorEmConta = valorEmConta;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public int getConta() {
		return conta;
	}

	public double getValorEmConta() {
		return valorEmConta;
	}

	public void deposito(double deposito) {
		this.deposito(deposito);
	}

	public void saque(double saque) {
		this.saque(saque);
	}

	public String toString() {
		return "Conta número " + conta + ", Proprietário: " + nome + ", Quantia: R$ " + valorEmConta;
	}
}
