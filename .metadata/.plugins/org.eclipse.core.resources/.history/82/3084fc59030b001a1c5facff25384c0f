import java.util.Scanner;

public class While {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		int x = sc.nextInt();

		int soma = 0;
		while (x != 0) {

		}
		while (x == 0) {
			System.out.println(soma);
		}
		soma += x;

		System.out.println(x);

		sc.close();
	}

}
