package entities;

public class Banco {

	private int conta;
	private String nome;
	private double valorEmConta;

	public Banco(int conta, String nome) {

		this.conta = conta;
		this.nome = nome;
	}

	public Banco(int conta, String nome, double valorEmConta) {

		this.conta = conta;
		this.nome = nome;
		this.valorEmConta = valorEmConta;
	}

}
