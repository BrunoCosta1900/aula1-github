package entities;

public class Bank {

	private int account;
	private String name;
	private double valueInAccount;

	public Bank(int account, String name) {

		this.account = account;
		this.name = name;
	}

	public Bank(int account, String name, double initialDeposit) {

		this.account = account;
		this.name = name;
		addMoney(initialDeposit);
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAccount() {
		return account;
	}

	public double getValueInAccount() {
		return valueInAccount;
	}

	public void addMoney (double addMoney) {
		this.valueInAccount += addMoney;
	}

	public void removeMoney(double removeMoney) {
		this.valueInAccount -= removeMoney + 5.0;
	}

	public String toString (){
		return "abraço";
	}
}
