import java.util.Scanner;

public class maiorNumero {

	public static void main(String[] args) {

		Scanner sc = new Scanner (System.in);
		
		int n = sc.nextInt();
		int m = sc.nextInt();
		int o = sc.nextInt();
		
		int maior = 0;
		if (n > maior) {
			maior = n;
		}
		else if (m > n){
			maior = m;
		}
		else {
			maior = o;
		}
		
		System.out.println("Maior = " +maior);
		
		sc.close();
	}

}
