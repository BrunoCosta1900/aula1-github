package util;

public class Matrizes {

	public Integer x;
	public int m;
	public int n;

	public Matrizes(Integer x) {

		this.x = x;

	}

	public Matrizes(Integer x, int m) {

		this.x = x;
		this.m = m;

	}

	public Matrizes(Integer x, int m, int n) {

		this.x = x;
		this.m = m;
		this.n = n;
	}

	public String toString() {
		return "Matrizes [x=" + x + "]";
	}

}
